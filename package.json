{
  "name": "electurbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "compile": "turbo run compile",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean:workspaces": "turbo clean"
  },
  "devDependencies": {
    "prettier": "^3.0.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=20.4.0"
  },
  "packageManager": "npm@9.8.1"
}
